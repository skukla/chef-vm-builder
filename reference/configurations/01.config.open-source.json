	},
	"custom_demo": {
		"structure": {
			"website": {
				"base": "luma.demo"
			}
		},
		"backup": "",
		"data_packs": {},
		"custom_modules": {}
	},
	"application": {
		"authentication": {
			"composer": {
				"public_key": "",
				"private_key": "",
				"github_token": ""
			},
			"commerce_services_connector": {
				"production_api_key": "",
				"project_id": "",
				"data_space_id": ""
			}
		},
		"options": {
			"family": "Open Source",
			"version": "2.4.2"
		},
		"build": {
			"action": "install",
			"sample_data": false,
			"hooks": {
				"warm_cache": true,
				"enable_media_gallery": true,
				"commands": []
			}
		}
	},
	"infrastructure": {
		"php": "7.4",
		"composer": "1.10.16",
		"elasticsearch": true,
		"mailhog": true,
		"samba": true
	},
	"provisioner": {
		"type": "chef_zero",
		"version": "latest",
		"nodes_path": "nodes",
		"environments_path": "environments",
		"roles_path": "roles",
		"cookbooks_path": "cookbooks"
	},
	"remote_machine": {
		"name": "Kukla-Machine",
