{
	"vm": {
		"ip": "192.168.57.12",
		"name": "Luma Demo",
		"hypervisor": "virtualbox"
	},
	"custom_demo": {
		"structure": {
			"website": {
				"base": "luma.demo",
				"custom_b2b_website": "custom-b2b.demo"
			},
			"store_view": {
				"custom_store_view_ca_fr": "ca-fr.luma.demo"
			}
		},
		"backup": "",
		"data_packs": {
			"base-b2c-website": {
				"name": "base-b2c-website",
				"repository_url": "base-b2c-website"
			},
			"custom-b2b-website": {
				"name": "custom-b2b-website",
				"repository_url": "custom-b2b-website"
			},
			"custom-store-view-ca-fr": {
				"name": "custom-store-view-ca-fr",
				"repository_url": "custom-store-view-ca-fr"
			}
		},
		"custom_modules": {
			"data-install": {
				"name": "magentoese/module-data-install",
				"version": "dev-beta",
				"repository_url": "https://github.com/PMET-public/module-data-install.git"
			},
			"sctools": {
				"name": "magentoese/module-sctools",
				"repository_url": "git@github.com:PMET-public/module-sctools.git"
			},
			"theme-customizer": {
				"name": "magentoese/module-themecustomizer",
				"repository_url": "git@github.com:PMET-public/module-themecustomizer.git"
			},
			"autofill": {
				"name": "magentoese/module-autofill",
				"repository_url": "git@github.com:PMET-public/module-autofill.git"
			},
			"kukla-custom-theme": {
				"name": "skukla/theme-frontend-custom",
				"repository_url": "git@github.com:skukla/theme-frontend-custom.git"
			},
			"product-recommendations": "magento/product-recommendations",
			"pagebuilder-product-recommendations": "magento/module-page-builder-product-recommendations"
		}
	},
	"application": {
		"authentication": {
			"authentication": {
				"composer": {
					"public_key": "",
					"private_key": "",
					"github_token": ""
				},
				"commerce_services_connector": {
					"production_api_key": "",
					"project_id": "",
					"data_space_id": ""
				}
			}
		},
		"options": {
			"family": "Commerce",
			"version": "2.4.2"
		},
		"build": {
			"action": "install",
			"sample_data": false,
			"patches": true,
			"hooks": {
				"warm_cache": true,
				"enable_media_gallery": true,
				"commands": []
			}
		},
		"settings": {
			"admin_password": "admin123"
		}
	},
	"infrastructure": {
		"php": "7.4",
		"composer": "latest",
		"database": "magento",
		"elasticsearch": true,
		"mailhog": true,
		"samba": true
	},
	"provisioner": {
		"type": "chef_zero",
		"version": "latest",
		"nodes_path": "nodes",
		"environments_path": "environments",
		"roles_path": "roles",
		"cookbooks_path": "cookbooks"
	},
	"remote_machine": {
		"name": "Kukla-Machine",
		"box": "bento/ubuntu-18.04",
		"cpus": 2,
		"memory": 8192,
		"vram": 16
	},
	"hypervisor": {
		"vmware": {
			"gui": true,
			"linked_clones": true,
			"eth0_pcislotnumber": "32",
			"eth1_pcislotnumber": "33"
		},
		"virtualbox": {
			"gui": false,
			"linked_clones": false,
			"remote_display": "off",
			"default_nic_type": "82543GC"
		}
	},
	"vagrant": {
		"plugins": {
			"all": ["vagrant-multi-hostsupdater"],
			"vmware": ["vagrant-vmware-desktop"],
			"virtualbox": []
		}
	}
}
