{
	"vm": {
		"ip": "192.168.56.12",
		"name": "Luma Demo",
		"hypervisor": "virtualbox"
	},
	"custom_demo": {
		"structure": {
			"websites": [
				{
					"site_code": "base",
					"site_url": "luma.demo",
					"stores": [
						{
							"store_code": "main_website_store",
							"root_category": "Default Category",
							"store_views": [
								{
									"store_view_code": "ca_fr",
									"store_view_url": "luma.ca"
								}
							]
						},
						{
							"store_code": "venia_store",
							"root_category": "Venia Catalog",
							"store_views": [
								{
									"store_view_code": "venia_us_en"
								}
							]
						}
					]
				},
				{
					"site_code": "b2b",
					"site_url": "b2b.luma.demo",
					"stores": [
						{
							"store_code": "b2b_store",
							"root_category": "B2B Catalog",
							"store_views": [
								{
									"store_view_code": "b2b_us_en"
								}
							]
						}
					]
				}
			]
		},
		"backup": "",
		"data_packs": [
			{
				"source": "base-b2c-website"
			}
		],
		"custom_modules": [
			{
				"name": "magentoese/module-data-install",
				"version": "dev-master",
				"source": "https://github.com/PMET-public/module-data-install.git"
			}
		]
	},
	"application": {
		"authentication": {
			"composer": {
				"public_key": "",
				"private_key": "",
				"github_token": ""
			}
		},
		"options": {
			"family": "Commerce",
			"version": "2.4.2-p1"
		},
		"build": {
			"action": "install",
			"sample_data": false,
			"patches": true,
			"hooks": {
				"warm_cache": false,
				"enable_media_gallery": true,
				"commands": []
			}
		},
		"settings": {
			"admin_password": "admin123"
		}
	},
	"infrastructure": {
		"php": "7.4",
		"composer": "latest",
		"elasticsearch": true,
		"mailhog": true,
		"samba": true
	},
	"provisioner": {
		"type": "chef_zero",
		"version": "16.13.16",
		"nodes_path": "nodes",
		"environments_path": "environments",
		"roles_path": "roles",
		"cookbooks_path": "cookbooks"
	},
	"remote_machine": {
		"name": "Kukla-Machine",
		"box": "bento/ubuntu-18.04",
		"cpus": 2,
		"memory": 8192,
		"vram": 16,
		"remote_display": "off"
	},
	"hypervisor": {
		"gui": false,
		"linked_clones": false,
		"default_nic_type": "82543GC"
	},
	"vagrant": {
		"plugins": ["vagrant-hostsupdater"]
	}
}
