{
	"vm": {
		"name": "Luma VM",
		"hypervisor": "virtualbox"
	},
	"custom_demo": {
		"structure": {
			"websites": [
				{
					"site_code": "base",
					"site_url": "luma.demo",
					"stores": [
						{
							"store_views": [
								{
									"store_view_code": "default"
								}
							]
						}
					]
				}
			]
		},
		"data_packs": [
			{
				"source": "starter",
				"data": [
					{
						"data_path": "base-b2c-website"
					}
				]
			}
		],
		"custom_modules": []
	},
	"application": {
		"authentication": {
			"composer": {
				"public_key": "",
				"private_key": "",
				"github_token": ""
			}
		},
		"options": {
			"version": "2.4.5-p1"
		},
		"build": {
			"action": "install",
			"restore": "separate",
			"sample_data": false,
			"patches": true,
			"hooks": {
				"warm_cache": false,
				"backup": false
			}
		},
		"settings": {
			"admin_password": "admin4tls"
		}
	},
	"remote_machine": {
		"cpus": "2",
		"memory": "8192"
	}
}