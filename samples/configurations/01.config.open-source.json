{
	"vm": {
		"ip": "192.168.57.12",
		"name": "Luma Demo",
		"provider": "virtualbox"
	},
	"custom_demo": {
		"structure": {
			"website": {
				"base": "luma.demo"
			}
		}
	},
	"application": {
		"authentication": {
			"composer": {
				"public_key": "",
				"private_key": "",
				"github_token": ""
			}
		},
		"options": {
			"family": "Open Source",
			"version": "2.4.1"
		},
		"build": {
			"action": "install",
			"sample_data": true,
			"hooks": {
				"warm_cache": true,
				"enable_media_gallery": true,
				"commands": []
			}
		}
	},
	"infrastructure": {
		"php": "7.4",
		"composer": "1.10.16",
		"webserver": "nginx",
		"elasticsearch": true,
		"mailhog": true,
		"samba": true
	},
	"provisioner": {
		"type": "chef_zero",
		"version": "latest",
		"nodes_path": "nodes",
		"environments_path": "environments",
		"roles_path": "roles",
		"cookbooks_path": "cookbooks"
	},
	"remote_machine": {
		"name": "Kukla-Machine",
		"box": "bento/ubuntu-18.04",
		"cpus": 2,
		"ram": 8192,
		"vram": 16,
		"provider": {
			"vmware": {
				"gui": true,
				"linked_clones": true,
				"eth0_pcislotnumber": 32,
				"eth1_pcislotnumber": 33
			},
			"virtualbox": {
				"gui": false,
				"linked_clones": false,
				"remote_display": "off",
				"default_nic_type": "82543GC",
				"auto_update": false
			}
		}
	},
	"vagrant": {
		"plugins": {
			"all": ["vagrant-multi-hostsupdater"],
			"vmware": ["vagrant-vmware-desktop"]
		}
	}
}
